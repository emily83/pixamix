    "express": "^4.17.1",
    "mongoose": "^5.9.18",
    "mongoose-simple-random": "^0.4.1",
    "morgan": "^1.10.0",
    "randomstring": "^1.1.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
